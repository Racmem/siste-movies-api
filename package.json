{
  "name": "siste_movies_api",
  "version": "0.0.1",
  "description": "Sistecredito Recruitment movies API",
  "main": "server.js",
  "engines": {
    "node": "16.15.0",
    "npm": "8.9.0"
  },
  "scripts": {
    "console": "node console.js",
    "cover": "npm run test -- --coverage",
    "test": "NODE_ENV=testing jest --ci --runInBand --detectOpenHandles --coverage",
    "test:inspect": "NODE_ENV=testing node --inspect --debug-brk jest",
    "eslint:check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint:diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint:fix": "eslint \"**/*.js\" --fix --ignore-pattern ./.eslintrc.js",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "migrations": "sequelize db:migrate",
    "migrations:test": "NODE_ENV=testing sequelize db:migrate",
    "start:dev": "nodemon -L --inspect server.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Racmem/siste-movies-api.git"
  },
  "keywords": [
    "sistecredito",
    "api",
    "node",
    "recruitment"
  ],
  "author": "Oscar F. Mu√±oz Lopez",
  "license": "MIT",
  "homepage": "",
  "cacheDirectories": [
    "node_modules"
  ],
  "jest-junit": {
    "outputDirectory": "./test",
    "outputName": "result_test.xml"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura",
      "json",
      "lcov",
      "clover"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!**/config/**",
      "!**/documentation/**",
      "!**/repositories/**",
      "!domain/usecase/file/**",
      "!**/driven-adapters/**",
      "!**/entry-points/**",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/test/**",
      "!server.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRunner": "jest-circus/runner"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff && npm audit"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.822.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "cognito-express": "^2.0.18",
    "convict": "^6.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.16.2",
    "express-validator": "^6.7.0",
    "express-wolox-logger": "1.1.0",
    "factory-girl": "^5.0.4",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.32",
    "multer": "^1.4.2",
    "node-cache": "^4.2.1",
    "nodemailer": "^6.6.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "ramda": "^0.26.1",
    "rollbar": "^2.19.4",
    "sequelize": "^5.22.5",
    "strict-transport-security": "^0.3.0",
    "swagger-ui-express": "^4.1.5",
    "table": "^5.4.6",
    "umzug": "^2.1.0",
    "yargs-parser": "^13.1.2"
  },
  "devDependencies": {
    "ajv": "^6.12.6",
    "aws-sdk": "^2.822.0",
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-es2015": "6.24.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.9.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-node": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest": "^24.0.0",
    "jest-circus": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jwt-simple": "^0.5.6",
    "nodemon": "^2.0.19",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "prompt": "^1.0.0",
    "sequelize-cli": "5.5.1",
    "supertest": "^4.0.2"
  }
}
